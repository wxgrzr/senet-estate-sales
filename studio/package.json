{
  "name": "senet-estate-sales-web_studio",
  "private": true,
  "version": "1.0.0",
  "main": "package.json",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "sanity dev",
    "start": "sanity start",
    "prebuild": "npm run extract-types",
    "build": "sanity build",
    "deploy": "sanity deploy",
    "deploy-graphql": "sanity graphql deploy",
    "extract-types": "sanity schema extract --enforce-required-fields"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/assist": "^3.2.0",
    "@sanity/google-maps-input": "^4.1.0",
    "@sanity/icons": "^3.5.0",
    "@sanity/vision": "^3.81.0",
    "date-fns": "^3.6.0",
    "nanoid": "^5.1.5",
    "pluralize-esm": "^9.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "^7.8.1",
    "sanity": "^3.88.3",
    "sanity-plugin-asset-source-unsplash": "^3.0.3",
    "speakingurl": "^14.0.1",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^4.0.0",
    "@types/react": "^18.3.12",
    "eslint": "^8.6.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "^5.6.3"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "quoteProps": "as-needed",
    "jsxSingleQuote": true
  }
}
