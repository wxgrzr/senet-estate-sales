{
  "name": "senet-estate-sales-web",
  "description": "Monorepo for Senet Estate Sales website using Sanity and Next.js hosted on Vercel",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:next": "npm run dev --workspace=_app",
    "dev:studio": "npm run dev --workspace=studio"
  },
  "workspaces": [
    "studio",
    "_app"
  ],
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "sonner": "^2.0.3"
  }
}
